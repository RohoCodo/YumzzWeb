{
  "hosting": {
    "public": "dist/yumzz",
    "headers": [
      {
        "source": "/apple-app-site-association",
        "headers": [{"key": "Content-Type", "value": "application/json"}]
      }
    ],
    "appAssociation": "AUTO",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/apple-app-site-association",
        "destination": "/apple-app-site-association"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "runtime": "nodejs14"
    }
  ]
}
